      <div class="banner--mountains" aria-hidden="true"></div>
    </header>

    <main>
      <div class="container">{{ content | safe }}</div>
    </main>

    <footer class="banner--FOOTER">
      <div class="container">
        <div class="split">
          <dl class="previous--events">
            <dt>Previously</dt>
            <dd>
              <a href="https://2021.pwasummit.org/" title="PWA Summit 2021">2021</a>
            </dd>
          </dl>
          <nav class="nav">
            {{ collections.all | eleventyNavigation('Main') | eleventyNavigationToHtml | safe }}
          </nav>
        </div>
        <div class="footer">
          <p><a href="https://twitter.com/PwaSummit" target="_blank">@PWASummit</a><br /></p>
          <p class="footer--links">
            Organized by <a target="_blank" href="https://www.google.com/chrome/">Google</a>,
            <a target="_blank" href="https://www.microsoft.com/en-us/edge">Microsoft</a>,
            <a target="_blank" href="https://developer.samsung.com/internet">Samsung Internet</a>,
            <a target="_blank" href="https://01.org/chromium">Intel</a>
          </p>
        </div>
        <div class="footer footer--SPONSORS">
          <p class="footer--links footer--sponsors">
            <span>Hosting sponsored by </span
            ><a target="_blank" href="https://www.netlify.com/"
              ><img src="/images/netlify.svg" class="footer--logo" alt="Netlify"
            /></a>
          </p>
        </div>
      </div>
    </footer>
    {% include "icons.njk" %}

{
  "name": "2021.pwasummit.org",
  "version": "1.0.0",
  "description": "Website for the 2021 PWA Summit",
  "main": "index.js",
  "scripts": {
    "start": "run-s vite",
    "prebuild": "run-s lint",
    "build": "NODE_ENV=production run-p *:build",
    "test": "run-s build",
    "lint": "run-p *:lint",
    "fix": "run-p *:fix",
    "js:lint": "eslint .",
    "js:fix": "eslint --fix .",
    "css:lint": "stylelint src/sass",
    "css:fix": "stylelint --fix src/sass",
    "prettier:lint": "prettier -c .",
    "prettier:fix": "prettier --write .",
    "md:lint": "remark -qf .",
    "vite": "vite",
    "vite:build": "vite build --emptyOutDir"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PWA-Summit/2021.pwasummit.org.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PWA-Summit/2021.pwasummit.org/issues"
  },
  "homepage": "https://github.com/PWA-Summit/2021.pwasummit.org#readme",
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.2.0",
    "@emmetio/expand-abbreviation": "^0.7.3",
    "eslint": "^7.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "markdown-it": "^12.0.6",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-figure": "^0.2.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-header-sections": "^1.0.0",
    "markdown-it-kbd": "^2.2.2",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-video": "^0.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "remark": "^13.0.0",
    "remark-cli": "^9.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-lint": "^8.0.0",
    "remark-lint-heading-increment": "^2.0.1",
    "remark-lint-prohibited-strings": "^2.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "sass": "^1.32.13",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "uslug": "^1.0.4",
    "vite": "^2.3.3",
    "vite-plugin-eleventy": "^0.0.4"
  }
}
